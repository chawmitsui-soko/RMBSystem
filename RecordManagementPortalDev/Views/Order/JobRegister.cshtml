		async function uploadfile()
		{
			var jobtype = $("input[name=optionsRad]:checked").val();
			var customer = document.getElementById("CustomerCode").value;
			var files = document.getElementById('FormFile').files;
			if(files.length==0){
				alert("Please choose any file...");
				return;
			}
			var filename = files[0].name;
			var extension = filename.substring(filename.lastIndexOf(".")).toUpperCase();
			if (extension == '.XLS' || extension == '.XLSX') {
				var jsonResult  =  await excelFileToJSON(files[0]);
					$.post("/Order/CheckRecordJRegisJson",
					{ customer: customer, jobtype: jobtype, data: JSON.stringify(jsonResult) },
						function (response) {
						//document.body.style.cursor = 'wait';
						//document.getElementById('savejob').style.cursor= 'wait';
						//document.getElementById('upload').style.cursor= 'wait';
						if (jobtype == 'D2')
