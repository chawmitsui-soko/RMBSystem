							if (response == 0)
							{
								alert("The record is not existed in warehouse.");
							}
							else
							{
								for (var a =0; a < jsonResult.Sheet1.length; a++)
								{
									var department = jsonResult.Sheet1[a].DeptCode;
									var recordno = jsonResult.Sheet1[a].RecordNo.toString();
									RetrieveFTCartons(department, recordno);
									array.push(department+recordno);
								}
							}
						}
						if (jobtype == 'C1')
